<OpenSearchDescription xmlns="http://a9.com/-/spec/opensearch/1.1/" xmlns:moz="http://www.mozilla.org/2006/browser/search/">
  <ShortName>PhraseFinder</ShortName>
  <Description>Search millions of books for language use</Description>
  <InputEncoding>UTF-8</InputEncoding>
  <Image width="16" height="16" type="image/png">http://phrasefinder.io/favicon16.png</Image>
  <Image width="16" height="16" type="image/x-icon">data:image/png;base64,</Image>
  <Url type="text/html" template="http://phrasefinder.io/phrases">
    <Param name="query" value="{searchTerms}" />
    <Param name="corpus" value="eng-us" />
  </Url>
  <moz:SearchForm>http://phrasefinder.io/</moz:SearchForm>
</OpenSearchDescription>
